	<style>
		.carousel1 {
			display: grid;
			margin: auto;
			position: relative;
			width: 95%;
			height: 500px;

		}


		.carousel-inner1 {
			display: flex;
			gap: 10px;
			width: calc(250*18);
			/* Space between items */
			animation: scroll 40s linear infinite;
		}

		.carousel-inner1:hover {
			animation-play-state: paused;
		}

		.carousel-item1 {
			/* Prevent items from shrinking or growing */
			width: 250px;
			/* Adjust width as needed */
			display: flex;
			align-items: center;
			padding: 5px;
			perspective: 100px;
		}

		.carousel-image {
			width: 100%;
			height: 280px;
			border-radius: 0px;
			transition: transform 1s;
		}

		.carousel-image:hover {
			transform: translateZ(20px);
		}

		@keyframes scroll {
			0% {
				transform: translateX(0);
			}

			100% {
				transform: translateX(calc(-250px * 9));
			}
		}

		/* Hide scrollbar for better UX */
		.carousel1::-webkit-scrollbar {
			display: none;
		}

		.carousel1 {
			-ms-overflow-style: none;
			/* Internet Explorer 10+ */
			scrollbar-width: none;
			/* Firefox */
		}

		/* Hide scrollbar for clean design */
		.carousel1::-webkit-scrollbar {
			display: none;
		}




