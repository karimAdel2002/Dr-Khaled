		<script>
			document.addEventListener("DOMContentLoaded", function() {
			const navbar = document.querySelector(".site-nav");
			const logo = document.querySelector(".logo");
			const navLinks = document.querySelectorAll(".site-nav a"); // All navbar links
			const dropdownLinks = document.querySelectorAll(".dropdown a"); // Dropdown links
		
			window.addEventListener("scroll", function() {
				if (window.scrollY > 50) {
					navbar.classList.add("scrolled");
					logo.src = "/images/dark-logo.png"; // Update logo on scroll
					// Change text color to black for all links (navbar and dropdown)
					navLinks.forEach(link => link.style.color = "black");
					dropdownLinks.forEach(link => link.style.color = "black");
				} else {
					navbar.classList.remove("scrolled");
					logo.src = "/images/New Project 3 [72E7506].png"; // Revert to original logo
					// Change text color back to white for all links (navbar and dropdown)
					navLinks.forEach(link => link.style.color = "white");
					dropdownLinks.forEach(link => link.style.color = "black");
				}
			});
		});
		
		
			</script>
		

    <script>
      // Image Slider Logic
      let currentSlide = 0;
      const images = [
          "/images/about_1.jpg",
          "/images/about_2.jpg"
      ];

      function changeSlide(direction) {
          currentSlide = (currentSlide + direction + images.length) % images.length;
          document.getElementById('slider-image').src = images[currentSlide];
      }

     // Function to initialize and hide empty tabs
    function initializeTabs() {
        const tabs = [
            { tab: "tab_1", content: "des_1" },
            { tab: "tab_2", content: "des_2" },
            { tab: "tab_3", content: "des_3" }
        ];

        tabs.forEach(({ tab, content }) => {
            const tabElement = document.getElementById(tab);
            const contentElement = document.getElementById(content);

            if (!tabElement || !contentElement) return;

            // Hide tab and content if the tab's content is empty
            if (!tabElement.innerHTML.trim()) {
                tabElement.style.display = "none";
                contentElement.style.display = "none";
            }
        });
    }

    // Tab Logic
    function showTab(tabId, event) {
        document.querySelectorAll('.tab-content').forEach(tab => {
            tab.style.display = 'none';
        });
        document.querySelectorAll('.tab').forEach(tab => {
            tab.classList.remove('active');
        });
        const contentElement = document.getElementById(tabId);
        if (contentElement) {
            contentElement.style.display = 'block';
        }
        if (event.target) {
            event.target.classList.add('active');
        }
    }

    // Initialize tabs on DOMContentLoaded
    document.addEventListener("DOMContentLoaded", initializeTabs);

</script>

